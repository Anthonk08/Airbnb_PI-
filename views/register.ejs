      <!--End navbar-->

        <div class="container">
            <div class="row">
                <div class="col-md-5 mx-auto">
                  <div id="first">
                     <div class="myform form ">
                           <div class="logo mb-3">
                              <div class="col-md-12 text-center">
                                 <h1>Reg&iacutestrate</h1>
                              </div>
                           </div>
                        <form action="/register" method="post" name="register">
                              <div class="form-group">
                                 <label for="exampleInputEmail1">Correo eléctronico</label>
                                 <input type="email" name="email"  class="form-control" id="emailHolder" aria-describedby="emailHelp" placeholder="Digite correo electrónico">
                              </div>
                              <div class="form-group">
                                 <p class="text-center">By signing up you accept our <a href="#">Terms Of Use</a></p>
                              </div>
                              <div class="col-md-12 text-center ">
                                 <button type="button" id="form" value="true" class=" btn btn-block mybtn btn-primary tx-tfm" data-toggle="modal" data-target="#myModal">Continuar</button>
                              </div>
                              <div class="col-md-12 ">
                                 <div class="login-or">
                                    <hr class="hr-or">
                                    <span class="span-or">o</span>
                                 </div>
                              </div>
                              <div class="col-md-12 mb-3">
                                 <p class="text-center">
                                    <a href="javascript:void();" class="google btn mybtn2"><i class="fa fa-google">
                                       </i> Continuar con Google
                                    </a>
                                 </p>
                              </div>
                              <div class="col-md-12 mb-3">
                                 <p class="text-center">
                                    <a href="javascript:void();" class="facebook btn mybtn2"><i class="fa fa-facebook">
                                       </i> Continuar con Facebook
                                    </a>
                                 </p>
                              </div>
                              <div class="col-md-12 mb-3">
                                 <p class="text-center">
                                    <a href="javascript:void();" class="apple btn mybtn2"><i class="fa fa-apple">
                                       </i> Continuar con Apple
                                    </a>
                                 </p>
                              </div>
                              <div class="col-md-12 mb-3">
                                 <p class="text-center">
                                    <a href="/register-phone" class="numero btn mybtn2"><i class="fa fa-mobile-phone">
                                       </i> Continuar con Número telefónico
                                    </a>
                                 </p>
                              </div>
                        </form>
                     </div>
                  </div>
                </div>
            </div>
          </div>

          <!-- Button trigger modal -->
         <script>
            $(document).ready(function() {
               $("#form").click(function(){
                  $("#email").val($("#emailHolder").val());
               }); 
            });
         </script>
         <% if(sessionSuccess==true){%>
            <script>
               $("#modal-message").modal();
            </script>
         <%}%>
