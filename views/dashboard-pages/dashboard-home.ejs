<!DOCTYPE html>
<!--=== Coding by CodingLab | www.codinglabweb.com === -->
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <!-- Bootstrap link -->
    <link
      rel="stylesheet"
      href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.1/css/bootstrap.min.css"
      integrity="sha384-WskhaSGFgHYWDcbwN70/dfYBj47jz9qbsMId/iRN3ewGhXQFZCSftd1LZCfmhktB"
      crossorigin="anonymous"
    />
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-Zenh87qX5JnK2Jl0vWa8Ck2rdkQ2Bzep5IDxbcnCeuOxjzrPF/et3URy9Bv1WTRi"
      crossorigin="anonymous"
    />
    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-OERcA2EqjJCMA+/3y+gxIOqMEjwtxJY7qPCqsdltbNJuaOe923+mo//f6V8Qbsw3"
      crossorigin="anonymous"
    ></script>
    <link
      rel="stylesheet"
      href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@20..48,100..700,0..1,-50..200"
    />

    <!----======== CSS ======== -->
    <link rel="stylesheet" href="/style/dashboard-pages/dashboard-home.css" />

    <!----===== Iconscout CSS ===== -->
    <link
      rel="stylesheet"
      href="https://unicons.iconscout.com/release/v4.0.0/css/line.css"
    />

    <title>Airbnb - Inicio</title>

    <!--Airbnb icon-->
    <link
      rel="shortcut icon"
      type="image/x-icon"
      href="/sources/Airbnb icon.ico"
    />
  </head>
  <body>
    <nav>
      <div class="logo-image">
        <a href="./home"><img src="/sources/Airbnb icon navbar.png" /></a>
      </div>

      <div class="menu-items">
        <ul class="nav-links">
          <li>
            <a href="/home">
              <i class="uil uil-estate"></i>
              <span class="link-name">Inicio</span>
            </a>
          </li>
          <li>
            <a href="/dashboard-profit">
              <i class="uil uil-credit-card"></i>
              <span class="link-name">Consultar usuarios</span>
            </a>
          </li>
          <li>
            <a href="/dashboard-booking">
              <i class="uil uil-chart"></i>
              <span class="link-name">Consultar pagos</span>
            </a>
          </li>
        </ul>
      </div>
    </nav>

    <section class="dashboard">
      <div class="header-nav">
        <!--Navbar Title-->
        <span class="title_name">Panel De Administraci&oacuten</span>

        <ul class="navbar-nav ml-auto navbar_move">
          <li class="nav-item dropdown">
            <a
              class="nav-link menu_property"
              href="#"
              role="button"
              data-bs-toggle="dropdown"
              aria-expanded="false"
            >
              <span class="material-symbols-outlined setting-menu">menu</span>
              <img
                class="rounded-circle image-nav"
                src="/sources/person-icon.png"
              />
            </a>

            <ul class="dropdown-menu dropdown_father">
              <li>
                <a class="dropdown-item dropdown_children" href="#"
                  >Administrador</a
                >
              </li>
              <hr class="change_line_navbar" />
              <li>
                <a class="dropdown-item dropdown_children" href="#"
                  >Cerrar Sesi&oacuten</a
                >
              </li>
            </ul>
          </li>
        </ul>
      </div>

      <div class="dash-content">
        <div class="overview">
          <div class="title">
            <i class="uil uil-tachometer-fast-alt"></i>
            <span class="text">Tablero Administrativo</span>
          </div>

          <div class="boxes">
            <div class="box box1">
              <i class="uil uil-grin"></i>
              <span class="text">Nuevos visitantes</span>
              <span class="number"><%=reportList[0]%></span>
            </div>
            <div class="box box1">
              <i class="uil uil-smile-beam"></i>
              <span class="text">Nuevos Anfitriones</span>
              <span class="number"><%=reportList[1]%></span>
            </div>
            <div class="box box1">
              <i class="uil uil-house-user"></i>
              <span class="text">Nuevos hospedajes</span>
              <span class="number"><%=reportList[2]%></span>
            </div>

            <div class="box box2">
              <i class="uil uil-money-bill"></i>
              <span class="text">Dinero ganado hoy</span>
              <span class="number">$<%=reportList[3]%>.00</span>
            </div>
            <div class="box box2">
              <i class="uil uil-money-withdraw"></i>
              <span class="text">Dinero ganado en la semana</span>
              <span class="number">$<%=reportList[4]%>.00</span>
            </div>
            <div class="box box2">
              <i class="uil uil-money-stack"></i>
              <span class="text">Dinero ganado en el mes</span>
              <span class="number">$<%=reportList[5]%>.00</span>
            </div>

            <div class="box box3">
              <i class="uil uil-user-check"></i>
              <span class="text">Reservaciones en el d&iacutea</span>
              <span class="number"><%=reportList[6]%></span>
            </div>
            <div class="box box3">
              <i class="uil uil-user-plus"></i>
              <span class="text">Reservaciones en la semana</span>
              <span class="number"><%=reportList[7]%></span>
            </div>
            <div class="box box3">
              <i class="uil uil-user-plus"></i>
              <span class="text">Reservaciones en el mes</span>
              <span class="number"><%=reportList[8]%></span>
            </div>
          </div>
        </div>

        <div class="activity">
          <div class="title">
            <i class="uil uil-clock-three"></i>
            <span class="text">Ultimas reservaciones</span>
          </div>

          <table class="table table-striped">
            <thead class="thead-dark">
              <tr>
                <th scope="col">Fecha de reservacion</th>
                <th scope="col">Fecha de retorno</th>
                <th scope="col">Alojamiento</th>
                <th scope="col">Monto pagado</th>
                <th scope="col">Estado</th>
              </tr>
            </thead>
            <tbody>
              <%if(recentRents!=''){%> <% recentRents.forEach(function(rent){ %>
              <tr>
                <td><%=rent.rental_date%></td>
                <td><%=rent.return_date%></td>
                <td>
                  <a
                    class="data-list"
                    href="/lodging-profile?id=<%=rent.id_property%>"
                    >Ver alojamiento</a
                  >
                </td>
                <td><strong>$<%=rent.amount%>.00</strong></td>
                <%if(rent.state==1){%>
                <td><strong>Disponible</strong></td>
                <%}else{%>
                <td><strong>No disponible</strong></td>
                <%}%>
              </tr>
              <%})%> <%}%>
            </tbody>
          </table>
        </div>
      </div>
    </section>

    <script>
      window.addEventListener("scroll", () => {
        const header = document.querySelector("header-nav");
        header.classList.toggle("ocultar-nav", window.scrollY > 0);
      });
    </script>
  </body>
</html>
