                    </div>

                    <div class="data joined">
                        <span class="data-title">EN EL MES</span>
                        <span class="data-list">$10,000,000</span>
                    </div>

                    <div class="data names">
                        <span class="data-title">EN EL AÃ‘O</span>
                        <span class="data-list">$100,000,000</span>
                    </div>
                </div>
            </div>
        </div>
        <canvas id="graphics1" width="400" height="400"></canvas>
    </section>

    <!-- Link de Chart -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/4.0.1/chart.min.js"></script>
    <script>
        // Ocultar titulo del navbar
        window.addEventListener("scroll", () => {
            const header = document.querySelector("header-nav");
            header.classList.toggle("ocultar-nav", window.scrollY>0);
        });
    </script>
    <script>
        // Graficas
        let miCanvas = document.getElementById("graphics1").getContext("2d");
        var graphics1 = new Chart(miCanvas, {
            type:"line",
            data: {
                labels:["Vino", "Tequila", "Cerveza", "Ron"],
                datasets:[
                    {
                        label:"Ganancias totales",
                        backgroundColor:"blue",
                        borderColor: "black",
                        data:[12,39,5,30]
                    }
                ]
            }
        });
    </script>
